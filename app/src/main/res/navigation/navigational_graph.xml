<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigational_graph"
    app:startDestination="@id/loginPageFragment">

    <fragment
        android:id="@+id/loginPageFragment"
        android:name="com.example.facebook.fragment.LoginPageFragment"
        android:label="LoginPageFragment"
        tools:layout="@layout/fragment_login_page">
        <action
            android:id="@+id/navigation_login_to_create_acc_join_page"
            app:destination="@id/createAccJoinPageFragment" />
        <action
            android:id="@+id/action_loginPageFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
        <!--        <argument-->
        <!--            android:name="username"-->
        <!--            android:defaultValue="@null"-->
        <!--            app:argType="String"-->
        <!--            app:nullable="true"-->
        <!--            />-->
        <!--        <argument-->
        <!--            android:name="password"-->
        <!--            app:argType="String"-->
        <!--            android:defaultValue="@null"-->
        <!--            app:nullable="true"/>-->

    </fragment>
    <fragment
        android:id="@+id/createAccJoinPageFragment"
        android:name="com.example.facebook.fragment.CreateAccJoinPageFragment"
        android:label="CreateAccJoinPageFragment"
        tools:layout="@layout/fragment_create_account_join_pg">
        <action
            android:id="@+id/nav_createAccJoinPageFragment_to_loginPageFragment"
            app:destination="@id/loginPageFragment" />
        <action
            android:id="@+id/nav_createAccJoinPageFragment_to_createUserNamePageFragment"
            app:destination="@id/createAccountNamePageFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountNamePageFragment"
        android:name="com.example.facebook.fragment.CreateAccountNamePageFragment"
        android:label="CreateAccountNamePageFragment"
        tools:layout="@layout/fragment_create_account_name_pag">
        <action
            android:id="@+id/nav_createAccUserNameFragment_to_createAccBDayFragment"
            app:destination="@id/createAccountBDayFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountBDayFragment"
        android:name="com.example.facebook.fragment.CreateAccountBDayFragment"
        android:label="CreateAccountBDayFragment"
        tools:layout="@layout/fragment_create_account_bday_page">
        <action
            android:id="@+id/nav_createAccBDayFragment_to_createAccGenderFragment"
            app:destination="@id/createAccountGenderFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountGenderFragment"
        android:name="com.example.facebook.fragment.CreateAccountGenderFragment"
        android:label="CreateAccountGenderFragment"
        tools:layout="@layout/fragment_create_account_gender_page">
        <action
            android:id="@+id/nav_createAccGenderFragment_to_createAccPhnumFragment"
            app:destination="@id/createAccountMobileNumFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountMobileNumFragment"
        android:name="com.example.facebook.fragment.CreateAccountMobileNumFragment"
        android:label="CreateAccountMobileNumFragment"
        tools:layout="@layout/fragment_create_account_phnum">
        <action
            android:id="@+id/nav_createAccPhNumFragment_to_createAccPswdFragment"
            app:destination="@id/createAccountPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountPasswordFragment"
        android:name="com.example.facebook.fragment.CreateAccountPasswordFragment"
        android:label="CreateAccountPasswordFragment"
        tools:layout="@layout/fragment_create_account_password">
        <action
            android:id="@+id/nav_createAccPswdFragment_to_createAccEmailFragment"
            app:destination="@id/createAccountEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountEmailFragment"
        android:name="com.example.facebook.fragment.CreateAccountEmailFragment"
        android:label="CreateAccountEmailFragment"
        tools:layout="@layout/fragment_create_account_mail">
        <action
            android:id="@+id/nav_createAccEmailFragment_to_createAccTermsFragment"
            app:destination="@id/createAccountTermsFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountTermsFragment"
        android:name="com.example.facebook.fragment.CreateAccountTermsFragment"
        android:label="CreateAccountTermsFragment"
        tools:layout="@layout/fragment_create_account_terms_privacy">
        <action
            android:id="@+id/action_createAccountTermsFragment_to_loginPageFragment"
            app:destination="@id/loginPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/profilePageFragment"
        android:name="com.example.facebook.fragment.ProfilePageFragment"
        android:label="ProfilePageFragment"
        tools:layout="@layout/fragment_profile_page" />
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.facebook.home.HomeFragment"
        android:label="HomeFragment" />


</navigation>